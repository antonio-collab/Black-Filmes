// prisma/schema.prisma

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

// Definir o modelo filme
model Filme {
  id          Int     @id @default(autoincrement())
  title       String
  imageURL    String
  amount      Int
  describe    String
  time_minutes Int
}
